# manager for one-liners.
pier

# manager for aliases.
aliases

# replacement for `ls` with prettier out format.
exa

# `tree` improved.
tre

# replacement for `find`. Includes `zsh` completion.
fd

# find duplicated files.
ddh

# replacement for `cat`.
bat

# csv file manipulator.
xsv

# find and replace text.
ruplacer

# filter unique records.
runiq

# code counter.
tokei

# manager for TODOs & annotations in a project. Includes `zsh` completion.
todor

# replacement for `ps`.
procs

# bandwidth usage.
bandwhich

# accounting tool.
ledger

# store cheatsheet
cheat

# concise examples
eg
