"*":
  "advanced-open-file":
    createDirectories: true
    createFileInstantly: true
    defaultInputValue: "Project root"
    fuzzyMatch: true
  "atom-beautify":
    bash:
      beautify_on_save: true
    coffeescript:
      beautify_on_save: true
    css: {}
    general:
      _analyticsUserId: "0c336756-a4c6-46e3-a3c2-b9ba5591b0f3"
    go:
      beautify_on_save: true
    haskell:
      beautify_on_save: true
    html:
      beautify_on_save: true
    java:
      beautify_on_save: true
    js:
      beautify_on_save: true
    json: {}
