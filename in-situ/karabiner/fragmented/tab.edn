         [:!WQRSequal_sign [:spacebar :period :!Sequal_sign :equal_sign :spacebar]]      ; ' .+= ' ;
         ; [:!WQRSopen_bracket []]                                                                 ;
         ; [:!WQRSclose_bracket []]                                                                ;
         ; [:!WQRSsemicolon []]                                                                    ;
         ; [:!WQRSquote []]                                                                        ;
