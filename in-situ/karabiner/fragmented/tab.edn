         ; [:!WQRScomma []]                                                                        ;
         ; [:!WQRSperiod []]                                                                       ;
         ; [:!WQRSslash []]                                                                        ;

         ; [:!WQRSdelete_or_backspace []]                                                          ;
         ; [:!WQRSreturn_or_enter []]                                                              ;
         ; [:!WQRSright_shift []]                                                                  ;
         [:!WQRSright_option [:spacebar :period :!S8 :equal_sign :spacebar]]             ; ' .*= ' ;
         [:!WQRSright_command [:spacebar :period :slash :equal_sign :spacebar]]          ; ' ./= ' ;
         ; [:!WQRSspacebar []]                                                                     ;

         ; [:!WQRSleft_command []]                                                                 ;
         ; [:!WQRSleft_option []]                                                                  ;
         ; [:!WQRSleft_control []]                                                                 ;
         ; [:!WQRSleft_shift []]                  NOT AVAILABLE                                    ;
         ; [:!WQRScaps_lock []]                                                                    ;
         ; [:!WQRStab []]                         NOT AVAILABLE                                    ;
         ; [:!WQRSnon_us_backslash []]                                                             ;
         ; [:!WQRSescape []]                                                                       ;
         ]}

