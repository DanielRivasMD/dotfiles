; tab
{:des "Tab Mode - alphabet"
 :rules
 [;
  [:!WQR#Pa [:semicolon :semicolon :a]]                                        ; 'α'               ;
  [:!WQR#Pb [:semicolon :semicolon :b]]                                        ; 'β'               ;
  ; [:!WQR#Pc [:semicolon :semicolon :c]]                                      ;                   ;
  [:!WQR#Pd [:semicolon :semicolon :d]]                                        ; 'δ'               ;
  [:!WQR#Pe [:semicolon :semicolon :e]]                                        ; 'ε'               ;
  [:!WQR#Pf [:semicolon :semicolon :f]]                                        ; 'φ'               ;
  [:!WQR#Pg [:semicolon :semicolon :g]]                                        ; 'γ'               ;
  [:!WQR#Ph [:semicolon :semicolon :h]]                                        ; 'η'               ;
  [:!WQR#Pi [:semicolon :semicolon :i]]                                        ; 'ι'               ;
  [:!WQR#Pj [:semicolon :semicolon :j]]                                        ; 'ξ'               ;
  [:!WQR#Pk [:semicolon :semicolon :k]]                                        ; 'κ'               ;
  [:!WQR#Pl [:semicolon :semicolon :l]]                                        ; 'λ'               ;
  [:!WQR#Pm [:semicolon :semicolon :m]]                                        ; 'μ'               ;
  [:!WQR#Pn [:semicolon :semicolon :n]]                                        ; 'ν'               ;
  [:!WQR#Po [:semicolon :semicolon :o]]                                        ; 'ο'               ;
  [:!WQR#Pp [:semicolon :semicolon :p]]                                        ; 'π'               ;
  ; [:!WQR#Pq [:semicolon :semicolon :q]]                                      ;                   ;
  [:!WQR#Pr [:semicolon :semicolon :r]]                                        ; 'ρ'               ;
  [:!WQR#Ps [:semicolon :semicolon :s]]                                        ; 'σ'               ;
  [:!WQR#Pt [:semicolon :semicolon :t]]                                        ; 'τ'               ;
  [:!WQR#Pu [:semicolon :semicolon :u]]                                        ; 'θ'               ;
  [:!WQR#Pv [:semicolon :semicolon :v]]                                        ; 'ω'               ;
  ; [:!WQR#Pw [:semicolon :semicolon :w]]                                      ;                   ;
  [:!WQR#Px [:semicolon :semicolon :x]]                                        ; 'χ'               ;
  [:!WQR#Py [:semicolon :semicolon :y]]                                        ; 'υ'               ;
  [:!WQR#Pz [:semicolon :semicolon :z]]                                        ; 'ζ'               ;
  ]}

