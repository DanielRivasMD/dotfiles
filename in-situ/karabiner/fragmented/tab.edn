; tab
{:des "Tab Mode - symbols"
 :rules
 [;
  [:!WQR#Phyphen [:spacebar :hyphen :equal_sign :spacebar]]                    ; ' -= '            ;
  [:!WQR#Pequal_sign [:spacebar :!Sequal_sign :equal_sign :spacebar]]          ; ' += '            ;
  [:!WQR#Popen_bracket [:!Scomma :!Ssemicolon]]                                ; '<:'              ;
  [:!WQR#Pclose_bracket [:!Speriod :!Ssemicolon]]                              ; '>:'              ;
  [:!WQR#Psemicolon [:!Ssemicolon :!Ssemicolon :!Scomma]]                      ; '::<'             ;
  ; [:!WQR#Pquote []]                                                          ;                   ;
  ; [:!WQR#Pbackslash []]                                                      ;                   ;
  [:!WQR#Pcomma [:spacebar :!Scomma :hyphen :spacebar]]                        ; ' <- '            ;
  [:!WQR#Pperiod [:spacebar :hyphen :!Speriod :spacebar]]                      ; ' -> '            ;
  [:!WQR#Pslash [:!S3 :!S1 :slash]]                                            ; '#!/'             ;

  ; [:!WQR#Pdelete_or_backspace []]                                            ;                   ;
  ; [:!WQR#Preturn_or_enter []]                                                ;                   ;
  ; [:!WQR#Pright_shift []]                                                    ;                   ;
  [:!WQR#Pright_option [:spacebar :!S8 :equal_sign :spacebar]]                 ; ' *= '            ;
  [:!WQR#Pright_command [:spacebar :slash :equal_sign :spacebar]]              ; ' /= '            ;
  ; [:!WQR#Pspacebar []]                                                       ;                   ;

  ; [:!WQR#Pleft_command []]                                                                       ;
  ; [:!WQR#Pleft_option []]                                                                        ;
  ; [:!WQR#Pleft_control []]                                                                       ;
  ; [:!WQR#Pleft_shift []]                                                                         ;
  ; [:!WQR#Pcaps_lock []]                                                                          ;
  ; [:!WQR#Ptab []]                                                                 NOT AVAILABLE  ;
  ; [:!WQR#Pnon_us_backslash []]                                                                   ;
  ; [:!WQR#Pescape []]                                                                             ;
  ]}

