; tab
{:des "Tab Mode - symbols"
 :rules [;
         [:!WQRhyphen [:spacebar :hyphen :equal_sign :spacebar]]               ; ' -= '            ;
         [:!WQRequal_sign [:spacebar :!Sequal_sign :equal_sign :spacebar]]     ; ' += '            ;
         [:!WQRopen_bracket [:!Scomma :!Ssemicolon]]                           ; '<:'              ;
         [:!WQRclose_bracket [:!Speriod :!Ssemicolon]]                         ; '>:'              ;
         [:!WQRsemicolon [:!Ssemicolon :!Ssemicolon :!Scomma]]                 ; '::<'             ;
         ; [:!WQRquote []]                                                                         ;
         ; [:!WQRbackslash []]                                                                     ;
         [:!WQRcomma [:spacebar :!Scomma :hyphen :spacebar]]                   ; ' <- '            ;
         [:!WQRperiod [:spacebar :hyphen :!Speriod :spacebar]]                 ; ' -> '            ;
         [:!WQRslash [:!S3 :!S1 :slash]]                                       ; '#!/'             ;

         ; [:!WQRdelete_or_backspace []]                                                           ;
         ; [:!WQRreturn_or_enter []]                                           ;                   ;
         ; [:!WQRright_shift []]                                               ;                   ;
         [:!WQRright_option [:spacebar :!S8 :equal_sign :spacebar]]            ; ' *= '            ;
         [:!WQRright_command [:spacebar :slash :equal_sign :spacebar]]         ; ' /= '            ;
         ; [:!WQRspacebar []]                                                  ;                   ;

         ; [:!WQRleft_command []]                                                                  ;
         ; [:!WQRleft_option []]                                                                   ;
         ; [:!WQRleft_control []]                                                                  ;
         ; [:!WQRleft_shift []]                                                                    ;
         ; [:!WQRcaps_lock []]                                                                     ;
         ; [:!WQRtab []]                         NOT AVAILABLE                                     ;
         ; [:!WQRnon_us_backslash []]                                                              ;
         ; [:!WQRescape []]                                                                        ;
         ]}

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; tab - shift
{:des "Tab Mode - symbols"
 :rules [;
