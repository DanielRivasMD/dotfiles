; tab
{:des "Tab Mode - alphabet"
 :rules [;
         [:!WQRa [:semicolon :semicolon :a]]     ; 'α'                                             ;
         [:!WQRb [:semicolon :semicolon :b]]     ; 'β'                                             ;
         ; [:!WQRc [:semicolon :semicolon :c]]   ;                                                 ;
         [:!WQRd [:semicolon :semicolon :d]]     ; 'δ'                                             ;
         [:!WQRe [:semicolon :semicolon :e]]     ; 'ε'                                             ;
         [:!WQRf [:semicolon :semicolon :f]]     ; 'φ'                                             ;
         [:!WQRg [:semicolon :semicolon :g]]     ; 'γ'                                             ;
         [:!WQRh [:semicolon :semicolon :h]]     ; 'η'                                             ;
         [:!WQRi [:semicolon :semicolon :i]]     ; 'ι'                                             ;
         [:!WQRj [:semicolon :semicolon :j]]     ; 'ξ'                                             ;
         [:!WQRk [:semicolon :semicolon :k]]     ; 'κ'                                             ;
         [:!WQRl [:semicolon :semicolon :l]]     ; 'λ'                                             ;
         [:!WQRm [:semicolon :semicolon :m]]     ; 'μ'                                             ;
         [:!WQRn [:semicolon :semicolon :n]]     ; 'ν'                                             ;
         [:!WQRo [:semicolon :semicolon :o]]     ; 'ο'                                             ;
         [:!WQRp [:semicolon :semicolon :p]]     ; 'π'                                             ;
         ; [:!WQRq [:semicolon :semicolon :q]]   ;                                                 ;
         [:!WQRr [:semicolon :semicolon :r]]     ; 'ρ'                                             ;
         [:!WQRs [:semicolon :semicolon :s]]     ; 'σ'                                             ;
         [:!WQRt [:semicolon :semicolon :t]]     ; 'τ'                                             ;
         [:!WQRu [:semicolon :semicolon :u]]     ; 'θ'                                             ;
         [:!WQRv [:semicolon :semicolon :v]]     ; 'ω'                                             ;
         ; [:!WQRw [:semicolon :semicolon :w]]   ;                                                 ;
         [:!WQRx [:semicolon :semicolon :x]]     ; 'χ'                                             ;
         [:!WQRy [:semicolon :semicolon :y]]     ; 'υ'                                             ;
         [:!WQRz [:semicolon :semicolon :z]]     ; 'ζ'                                             ;
         ]}

