         [:!TCc :!Of14 [:term]]                                                                    ;
         [:!TCd :!Of15 [:term]]                                                                    ;
         [:!TCe :!Of16 [:term]]                                                                    ;
         [:!TCf :!Of17 [:term]]                                                                    ;
         [:!TCg :!Of18 [:term]]                                                                    ;
         [:!TCh :!Of19 [:term]]                                                                    ;
         [:!TCi :!Of20 [:term]]                                                                    ;
         [:!TCj :!Tf11 [:term]]                                                                    ;
         [:!TCk :!Tf12 [:term]]                                                                    ;
         [:!TCl :!Tf14 [:term]]                                                                    ;
         [:!TCm :!Tf15 [:term]]                                                                    ;
         [:!TCn :!Tf16 [:term]]                                                                    ;
         [:!TCo :!Tf17 [:term]]                                                                    ;
         [:!TCp :!Tf18 [:term]]                                                                    ;
         [:!TCq :!Tf19 [:term]]                                                                    ;
         [:!TCr :!Tf20 [:term]]                                                                    ;
         [:!TCs :!OTf11 [:term]]                                                                   ;
         [:!TCt :!OTf12 [:term]]                                                                   ;
         [:!TCu :!OTf14 [:term]]                                                                   ;
         [:!TCv :!OTf15 [:term]]                                                                   ;
         [:!TCw :!OTf16 [:term]]                                                                   ;
         [:!TCx :!OTf17 [:term]]                                                                   ;
         [:!TCy :!OTf18 [:term]]                                                                   ;
