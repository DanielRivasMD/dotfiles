; Alacritty
{:des "Alacritty - Control - Command Mode"
 :rules [;
         ; [:!TChyphen :]                        ;                                                 ;
         ; [:!TCequal_sign :]                    ;                                                 ;
         [:!TCopen_bracket :!Of13]               ; helix comments                                  ;
         [:!TCclose_bracket :!Of14]              ; helix comments                                  ;
         [:!TCsemicolon :!Of15 [:term]]          ; helix jump                                      ;
         [:!TCquote :!Of16 [:term]]              ; helix jump                                      ;
         [:!TCbackslash :!Of17 [:term]]          ; helix jump                                      ;
         [:!TCcomma :!Of18]                      ; helix diagnostics                               ;
         [:!TCperiod :!Of19]                     ; helix diagnostics                               ;
         [:!TCslash :!Of20]                      ; helix diagnostics                               ;

         [:!TCdelete_or_backspace :!Sf1 [:term]] ; broot remove & helix symbols                    ;
         [:!TCreturn_or_enter :!Sf2 [:term]]     ; broot create & helix jump                       ;
         [:!TCright_shift :!Sf4 [:term]]         ; helix split                                     ;
         [:!TCright_option :!Sf5 [:term]]        ; helix split                                     ;
         [:!TCright_command :!Sf6 [:term]]       ; helix split                                     ;
         [:!TCspacebar :f4 [:term]]              ; broot viewer & helix tab                        ;

         ; [:!TCleft_command []]                                                                   ;
         ; [:!TCleft_option []]                                                                    ;
         ; [:!TCleft_control []]                                                                   ;
         ; [:!TCleft_shift []]                                                                     ;
         ; [:!TCcaps_lock []]                                                                      ;
         ; [:!TCtab []]                                                                            ;
         ; [:!TCnon_us_backslash []]                                                               ;
         ; [:!TCescape []]                                                                         ;
         ]}

