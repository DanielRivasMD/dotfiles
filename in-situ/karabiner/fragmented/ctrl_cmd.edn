; Alacritty
{:des "Alacritty - Keypad"
 :rules [;
         [:!TCkeypad_slash :!Of18 [:term]]       ; helix diagnostics                               ;
         [:!TCkeypad_asterisk :!Of19 [:term]]    ; helix diagnostics                               ;
         [:!TCkeypad_hyphen :!Of20 [:term]]      ; helix diagnostics                               ;
         [:!TCkeypad_plus :!Of13 [:term]]        ; helix comments                                  ;
         [:!TCkeypad_enter :!Of14 [:term]]       ; helix comments                                  ;
         [:!TCkeypad_period :!Sf2 [:term]]       ; helix jump                                      ;

         [:!TCkeypad_0 :!Sf1 [:term]]            ; helix symbols                                   ;
         [:!TCkeypad_1 :f13 [:term]]             ; helix symbols                                   ;
         [:!TCkeypad_2 :f14 [:term]]             ; helix symbols                                   ;
         [:!TCkeypad_3 :!Of15 [:term]]           ; helix jump                                      ;
         [:!TCkeypad_4 :f15 [:term]]             ; helix symbols                                   ;
         [:!TCkeypad_5 :f16 [:term]]             ; helix symbols                                   ;
         [:!TCkeypad_6 :!Of16 [:term]]           ; helix jump                                      ;
         [:!TCkeypad_7 :f17 [:term]]             ; helix symbols                                   ;
         [:!TCkeypad_8 :f18 [:term]]             ; helix symbols                                   ;
         [:!TCkeypad_9 :!Of17 [:term]]           ; helix jump                                      ;
         ]}

