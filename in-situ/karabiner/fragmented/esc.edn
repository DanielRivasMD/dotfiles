; esc
{:des "Escape Mode - alphabet"
 :rules
 [;
  ; [:!EWR#Pa [:!S2 :spacebar]]                                                ;                   ;
  ; [:!EWR#Pb [:!S2 :spacebar]]                                                ;                   ;
  [:!EWR#Pc [:!S2 :c :h :a :i :n :spacebar]]                                   ; '@chain '         ;
  [:!EWR#Pd [:!S2 :d :e :b :u :g :spacebar]]                                   ; '@debug '         ;
  [:!EWR#Pe [:!S2 :e :d :i :t :spacebar]]                                      ; '@edit '          ;
  ; [:!EWR#Pf [:!S2 :spacebar]]                                                ;                   ;
  ; [:!EWR#Pg [:!S2 :spacebar]]                                                ;                   ;
  ; [:!EWR#Ph [:!S2 :spacebar]]                                                ;                   ;
  [:!EWR#Pi [:!S2 :i :n :f :o :spacebar]]                                      ; '@info '          ;
  ; [:!EWR#Pj [:!S2 :spacebar]]                                                ;                   ;
  [:!EWR#Pk [:!S2 :c :l :i :c :k :period]]                                     ; '@click.'         ;
  [:!EWR#Pl [:!S2 :l :e :s :s :spacebar]]                                      ; '@less '          ;
  ; [:!EWR#Pm [:!S2 :spacebar]]                                                ;                   ;
  ; [:!EWR#Pn [:!S2 :spacebar]]                                                ;                   ;
  ; [:!EWR#Po [:!S2 :spacebar]]                                                ;                   ;
  [:!EWR#Pp [:!S2 :p :i :p :e :spacebar]]                                      ; '@pipe '          ;
  ; [:!EWR#Pq [:!S2 :spacebar]]                                                ;                   ;
  ; [:!EWR#Pr [:!S2 :spacebar]]                                                ;                   ;
  [:!EWR#Ps [:!S2 :s :h :o :w :spacebar]]                                      ; '@show '          ;
  [:!EWR#Pt [:!S2 :t :i :m :e :spacebar]]                                      ; '@time '          ;
  ; [:!EWR#Pu [:!S2 :spacebar]]                                                ;                   ;
  [:!EWR#Pv [:!S2 :e :v :a :l :spacebar]]                                      ; '@eval '          ;
  [:!EWR#Pw [:!S2 :w :a :r :n :spacebar]]                                      ; '@warn '          ;
  ; [:!EWR#Px [:!S2 :spacebar]]                                                ;                   ;
  ; [:!EWR#Py [:!S2 :spacebar]]                                                ;                   ;
  [:!EWR#Pz [:!S2 :w :h :i :c :h :spacebar]]                                   ; '@which '         ;
  ; [:!EWR#Pkeypad_num_lock [:!S2 :spacebar]]                                  ;                   ;
  ]}

