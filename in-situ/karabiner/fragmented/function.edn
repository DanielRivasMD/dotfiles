; function
{:des "Function Mode - symbols"
 :rules [;
         [:!Fhyphen [:hyphen :hyphen]]                     ; '--'                                  ;
         [:!Fequal_sign [:!Sequal_sign :!Sequal_sign]]     ; '++'                                  ;
         ; [:!Fopen_bracket []]                                                                    ;
         ; [:!Fclose_bracket []]                                                                   ;
         [:!Fsemicolon [:!Ssemicolon :!Ssemicolon]]        ; '::'                                  ;
         ; [:!Fquote []]                                                                           ;
         ; [:!Fbackslash []]                                                                       ;
         ; [:!Fcomma []]                                                                           ;
         ; [:!Fperiod []]                                                                          ;
         ; [:!Fslash []]                                                                           ;

         ; [:!Fdelete_or_backspace []]                                                             ;
         [:!Freturn_or_enter [:equal_sign :equal_sign]]    ; '=='                                  ;
         [:!Fright_shift [:!S1 :!S1]]                      ; '!!'                                  ;
         [:!Fright_option [:!S8 :!S8]]                     ; '**'                                  ;
         [:!Fright_command [:slash :slash]]                ; '//'                                  ;
         ; [:!Fspacebar []]                                                                        ;

         ; [:!Fleft_command []]                                                                    ;
         ; [:!Fleft_option []]                                                                     ;
         ; [:!Fleft_control []]                                                                    ;
         ; [:!Fleft_shift []]                                                                      ;
         ; [:!Fcaps_lock []]                                                                       ;
         ; [:!Ftab []]                                                                             ;
         ; [:!Fnon_us_backslash []]              NOT AVAILABLE                                     ;
         ; [:!Fescape []]                                                                          ;
         ]}

