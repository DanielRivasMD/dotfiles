         [:!Fk [:comma :comma :k]]     ; 'ϙ'                                                       ;
         [:!Fl [:comma :comma :l]]     ; 'ł'                                                       ;
         [:!Fm [:comma :comma :m]]     ; 'ɱ'                                                       ;
         [:!Fn [:comma :comma :n]]     ; 'ŋ'                                                       ;
