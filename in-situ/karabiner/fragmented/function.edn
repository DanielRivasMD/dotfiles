         [:!Fu [:comma :comma :u]]     ; 'Θ'                                                       ;
         [:!Fv [:comma :comma :v]]     ; 'Ω'                                                       ;
         [:!Fw [:comma :comma :w]]     ; 'ʍ'                                                       ;
         [:!Fx [:comma :comma :x]]     ; '∇'                                                       ;
         [:!Fy [:comma :comma :y]]     ; 'Λ'                                                       ;
         [:!Fz [:comma :comma :z]]     ; '∫'                                                       ;
         ]}

