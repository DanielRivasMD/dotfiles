; Alacritty
{:des "Alacritty - Control - Command Mode"
 :rules
 [;
  [:!TC#Phyphen :!Sf11 [:term]]                                                                    ;
  [:!TC#Pequal_sign :!Sf12 [:term]]                                                                ;
  [:!TC#Popen_bracket :!Sf13 [:term]]                                                              ;
  [:!TC#Pclose_bracket :!Sf14 [:term]]                                                             ;
  [:!TC#Psemicolon :!Sf18 [:term]]                                                                 ;
  [:!TC#Pquote :!Sf19 [:term]]                                                                     ;
  [:!TC#Pbackslash :!Sf20 [:term]]                                                                 ;
  [:!TC#Pcomma :f18 [:term]]                                                                       ;
  [:!TC#Pperiod :f19 [:term]]                                                                      ;
  [:!TC#Pslash :f20 [:term]]                                                                       ;

  [:!TC#Pdelete_or_backspace :!Sf1 [:term]]                                                        ;
  [:!TC#Preturn_or_enter :!Sf2 [:term]]                                                            ;
  [:!TC#Pright_shift :!Sf4 [:term]]                                                                ;
  [:!TC#Pright_option :!Sf5 [:term]]                                                               ;
  [:!TC#Pright_command :!Sf6 [:term]]                                                              ;
