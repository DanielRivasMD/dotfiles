; joker
{:des "Joker Mode - symbols"
 :rules
 [;
  ; [:!OTC#Phyphen []]                                                                             ;
  ; [:!OTC#Pequal_sign []]                                                                         ;
  [:!OTC#Popen_bracket [:spacebar :!S5 :!Scomma :!Sequal_sign :!S5 :spacebar]] ; ' %<+% '          ;
  [:!OTC#Pclose_bracket [:spacebar :!S5 :!Sequal_sign :!Speriod :!S5 :spacebar]];' %+>% '          ;
  [:!OTC#Psemicolon [:spacebar :!S5 :!S4 :!S5 :spacebar]]                      ; ' %$% '           ;
  [:!OTC#Pquote [:spacebar :!S5 :!St :!Speriod :!S5 :spacebar]]                ; ' %T>% '          ;
  [:!OTC#Pbackslash [:spacebar :!S5 :!Scomma :!Speriod :!S5 :spacebar]]        ; ' %<>% '          ;
  [:!OTC#Pcomma [:spacebar :!S5 :!Scomma :!S5 :spacebar]]                      ; ' %<% '           ;
  [:!OTC#Pperiod [:spacebar :!S5 :!Speriod :!S5 :spacebar]]                    ; ' %>% '           ;
  [:!OTC#Pslash [:spacebar :!S5 :!S1 :!Speriod :!S5 :spacebar]]                ; ' %!>% '          ;

  [:!OTC#Pdelete_or_backspace [:!Ssemicolon :i :n :!Ssemicolon]]               ; ' âˆˆ '             ;
  [:!OTC#Preturn_or_enter [:spacebar :!Sbackslash :!Speriod :spacebar]]        ; ' |> '            ;
  [:!OTC#Pright_shift [:spacebar :equal_sign :!Speriod :spacebar]]             ; ' => '            ;
  [:!OTC#Pright_option [:spacebar :!Ssemicolon :equal_sign :spacebar]]         ; ' := '            ;
  ; [:!OTC#Pright_command []]                                                                      ;
  ; [:!OTC#Pspacebar []]                                                                           ;

  ; [:!OTC#Pleft_command []]                                                        UNMAPPED       ;
  ; [:!OTC#Pleft_option []]                                                         UNMAPPED       ;
  ; [:!OTC#Pleft_control []]                                                        UNMAPPED       ;
  ; [:!OTC#Pleft_shift []]                                                          UNMAPPED       ;
  ; [:!OTC#Pcaps_lock []]                                                           NOT AVAILABLE  ;
  ; [:!OTC#Ptab []]                                                                 UNMAPPED       ;
  ; [:!OTC#Pnon_us_backslash []]                                                    UNMAPPED       ;
  ; [:!OTC#Pescape []]                                                              UNMAPPED       ;
  ]}

