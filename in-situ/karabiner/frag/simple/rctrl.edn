; control
{:des "Control Mode - symbols"
 :rules
 [;
  [:!W#Phyphen [:spacebar :hyphen :equal_sign :spacebar]]                      ; ' -= '            ;
  [:!W#Pequal_sign [:spacebar :!Sequal_sign :equal_sign :spacebar]]            ; ' += '            ;
  [:!W#Popen_bracket [:!S3 :open_bracket]]                                     ; '#['              ;
  [:!W#Pclose_bracket [:close_bracket :spacebar]]                              ; '] '              ;
  [:!W#Psemicolon [:semicolon :spacebar]]                                      ; '; '              ;
  ; [:!W#Pquote []]                                                                                ;
  ; [:!W#Pbackslash []]                                                                            ;
  [:!W#Pcomma [:comma :spacebar]]                                              ; ', '              ;
  [:!W#Pperiod [:period :period]]                                              ; '..'              ;
  [:!W#Pslash [:slash :slash :slash :spacebar]]                                ; '/// '            ;

  ; [:!W#Pdelete_or_backspace []]                                                                  ;
  ; [:!W#Preturn_or_enter []]                                                                      ;
  ; [:!W#Pright_shift []]                                                                          ;
  [:!W#Pright_option [:spacebar :!S8 :equal_sign :spacebar]]                   ; ' *= '            ;
  [:!W#Pright_command [:spacebar :slash :equal_sign :spacebar]]                ; ' /= '            ;
