;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; control
{:des "Control Mode - symbols"
 :rules
 [;
  [:!W#Popen_bracket [:!S3 :open_bracket]]                                     ; '#['              ;
  [:!W#Pclose_bracket [:close_bracket :spacebar]]                              ; '] '              ;
  [:!W#Psemicolon [:semicolon :spacebar]]                                      ; '; '              ;
  ; [:!W#Pquote []]                                                                                ;
  ; [:!W#Pbackslash []]                                                                            ;
  [:!W#Pcomma [:comma :spacebar]]                                              ; ', '              ;
  ; [:!W#Pperiod []]                                                                               ;
  [:!W#Pslash [:slash :slash :!S1 :spacebar]]                                  ; '//! '            ;

  [:!W#Pdelete_or_backspace [:spacebar :hyphen]]                               ; ' -'              ;
  [:!W#Preturn_or_enter [:!OTf11]]                                                                 ;
  [:!W#Pright_shift [:period :period]]                                         ; '..'              ;
  [:!W#Pright_option [:spacebar :!S8 :equal_sign :spacebar]]                   ; ' *= '            ;
  [:!W#Pright_command [:spacebar :slash :equal_sign :spacebar]]                ; ' /= '            ;
  [:!W#Pspacebar [:!OTf12]]                                                                        ;

  ; [:!W#Pleft_command []]                                                                         ;
  ; [:!W#Pleft_option []]                                                                          ;
  ; [:!W#Pleft_control []]                                                                         ;
  ; [:!W#Pleft_shift []]                                                                           ;
  ; [:!W#Pcaps_lock []]                                                                            ;
  ; [:!W#Ptab []]                                                                                  ;
  ; [:!W#Pnon_us_backslash []]                                                                     ;
  ; [:!W#Pescape []]                                                                               ;
  ]}

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; control
{:des "Control Mode - symbols"
 :rules
 [;
  [:!WS#Popen_bracket [:!S4 :!Sopen_bracket]]                                  ; '${'              ;
  [:!WS#Pclose_bracket [:!Sclose_bracket :spacebar]]                           ; '} '              ;
  [:!WS#Psemicolon [:!Ssemicolon :spacebar]]                                   ; ': '              ;
  ; [:!WS#Pquote []]                                                                               ;
  [:!WS#Pbackslash [:!OTf14]]                                                                      ;
  [:!WS#Pcomma [:period :spacebar]]                                            ; '. '              ;
  ; [:!WS#Pperiod []]                                                                              ;
  [:!WS#Pslash [:!S3 :!S1 :slash]]                                             ; '#!/'             ;

  [:!WS#Pdelete_or_backspace [:spacebar :hyphen :hyphen]]                      ; ' --'             ;
  [:!WS#Preturn_or_enter [:!OTSf11]]                                                               ;
  [:!WS#Pright_shift [:period :period :period]]                                ; '...'             ;
  [:!WS#Pright_option [:spacebar :period :!S8 :equal_sign :spacebar]]          ; ' .*= '           ;
  [:!WS#Pright_command [:spacebar :period :slash :equal_sign :spacebar]]       ; ' ./= '           ;
  [:!WS#Pspacebar [:!OTSf12]]                                                                      ;

  ; [:!WS#Pleft_command []]                                                                        ;
  ; [:!WS#Pleft_option []]                                                                         ;
  ; [:!WS#Pleft_control []]                                                                        ;
  ; [:!WS#Pleft_shift []]                                                           NOT AVAILABLE  ;
  ; [:!WS#Pcaps_lock []]                                                                           ;
  ; [:!WS#Ptab []]                                                                                 ;
  ; [:!WS#Pnon_us_backslash []]                                                                    ;
  ; [:!WS#Pescape []]                                                                              ;
  ]}

