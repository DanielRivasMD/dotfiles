; control
{:des "Control Mode - alphabet"
 :rules
 [;
  ; [:!W#Pa []]                                                                ;                   ;
  [:!W#Pb [:b :e :g :i :n]]                                                    ; 'begin'           ;
  ; [:!W#Pc []]                                                                ;                   ;
  ; [:!W#Pd []]                                                                ;                   ;
  [:!W#Pe [:e :n :u :m]]                                                       ; 'enum'            ;
  [:!W#Pf [:f :u :n :c :t :i :o :n]]                                           ; 'function'        ;
  ; [:!W#Pg []]                                                                ;                   ;
  ; [:!W#Ph []]                                                                ;                   ;
  ; [:!W#Pi []]                                                                ;                   ;
  ; [:!W#Pj []]                                                                ;                   ;
  ; [:!W#Pk []]                                                                ;                   ;
  ; [:!W#Pl []]                                                                ;                   ;
  [:!W#Pm [:m :u :t :a :b :l :e]]                                              ; 'mutable'         ;
  [:!W#Pn [:e :n :d]]                                                          ; 'end'             ;
  ; [:!W#Po []]                                                                ;                   ;
  [:!W#Pp [:p :l :o :t]]                                                       ; 'plot'            ;
  ; [:!W#Pq []]                                                                ;                   ;
  ; [:!W#Pr []]                                                                ;                   ;
  [:!W#Ps [:s :t :r :u :c :t]]                                                 ; 'struct'          ;
  [:!W#Pt [:t :y :p :e]]                                                       ; 'type'            ;
  ; [:!W#Pu []]                                                                ;                   ;
  ; [:!W#Pv []]                                                                ;                   ;
  [:!W#Pw [:w :h :e :r :e :spacebar]]                                          ; 'where '          ;
  [:!W#Px [:e :x :i :t]]                                                       ; 'exit'            ;
  ; [:!W#Py []]                                                                ;                   ;
  [:!W#Pz [:w :h :i :c :h :spacebar]]                                          ; 'which '          ;
  ; [:!W#Pright_control []]                                                         NOT AVAILABLE  ;
  ]}

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; control
{:des "Control Mode - symbols"
 :rules
 [;
  [:!W#Phyphen [:spacebar :hyphen :equal_sign :spacebar]]                      ; ' -= '            ;
  [:!W#Pequal_sign [:spacebar :!Sequal_sign :equal_sign :spacebar]]            ; ' += '            ;
  [:!W#Popen_bracket [:!S3 :open_bracket]]                                     ; '#['              ;
  [:!W#Pclose_bracket [:close_bracket :spacebar]]                              ; '] '              ;
  [:!W#Psemicolon [:semicolon :spacebar]]                                      ; '; '              ;
  ; [:!W#Pquote []]                                                            ;                   ;
  ; [:!W#Pbackslash []]                                                        ;                   ;
  [:!W#Pcomma [:comma :spacebar]]                                              ; ', '              ;
  ; [:!W#Pperiod []]                                                           ;                   ;
  [:!W#Pslash [:slash :slash :slash :spacebar]]                                ; '/// '            ;

  ; [:!W#Pdelete_or_backspace []]                                              ;                   ;
  [:!W#Preturn_or_enter [:spacebar :hyphen]]                                   ; ' -'              ;
  [:!W#Pright_shift [:period :period]]                                         ; '..'              ;
  [:!W#Pright_option [:spacebar :!S8 :equal_sign :spacebar]]                   ; ' *= '            ;
  [:!W#Pright_command [:spacebar :slash :equal_sign :spacebar]]                ; ' /= '            ;
  [:!W#Pspacebar :!OTf20 [:term]]                                              ;                   ;

  ; [:!W#Pleft_command []]                                                          UNMAPPED       ;
  ; [:!W#Pleft_option []]                                                           UNMAPPED       ;
  ; [:!W#Pleft_control []]                                                          UNMAPPED       ;
  ; [:!W#Pleft_shift []]                                                            UNMAPPED       ;
  ; [:!W#Pcaps_lock []]                                                             UNMAPPED       ;
  ; [:!W#Ptab []]                                                                   UNMAPPED       ;
  ; [:!W#Pnon_us_backslash []]                                                      UNMAPPED       ;
  ; [:!W#Pescape []]                                                                UNMAPPED       ;
  ]}

