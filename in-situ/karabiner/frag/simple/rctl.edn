; control
{:des "Control Mode - symbols"
 :rules
 [;
  [:!WS#Phyphen [:spacebar :period :hyphen :equal_sign :spacebar]]             ; ' .-= '           ;
  [:!WS#Pequal_sign [:spacebar :period :!Sequal_sign :equal_sign :spacebar]]   ; ' .+= '           ;
  [:!WS#Popen_bracket [:!S#P4 :!Sopen_bracket]]                                ; '${'              ;
  [:!WS#Pclose_bracket [:!Sclose_bracket :spacebar]]                           ; '} '              ;
